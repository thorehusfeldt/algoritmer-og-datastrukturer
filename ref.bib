@TechReport{Zelikovsky96a,
  author =       "A. Zelikovsky",
  title =        "Improved Approximation of Maximun Planar Subgraph",
  institution =  "Department of Computer Science, University of
                 Virginia",
  number =       "CS-96-09",
  month =        may # " 9",
  year =         "1996",
  keywords =     "Combinatorial problems, approximation algorithms,
                 planar subgraph",
  url =          "ftp://ftp.cs.virginia.edu/pub/techreports/CS-96-09.ps.Z",
  abstract =     "The maximum planar subgraph problem (MPSP) asks for a
                 planar subgraph of a given graph with the maximum total
                 cost. We suggest a new approximation algorithm for the
                 weighted MPSP. We show that it has performance ratio of
                 5/12 in the case of graphs where the maxumum cost of an
                 edge is at most twice the minimum cost of an edge. For
                 the variant of MPSP that asks for an outerplanar graph,
                 the algorithm suggested is the first with the higher
                 performance ratio (7/12 instead of 1/2).",
  annote =         "Tue, 11 Jun 1996 04:07:03 GMT"
}

                  
