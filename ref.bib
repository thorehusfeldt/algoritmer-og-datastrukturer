@Article{Fleischer$^+$93,
  author =       "R. Fleischer and B. P. Sinha and C. Uhrig",
  title =        "A Lower Bound for the Worst Case of
                 {Bottom-Up-Heapsort}",
  journal =      "Information and Computation",
  volume =       "2",
  pages =        "263--279",
  year =         "1993",
  annote =       "Abstract. Bottum-Up-Heapsort is a variant of Heapsort.
                 Until now, its worst case complexity for the number of
                 comparisons has been known to be bounded above by
                 {$1.5n \log n + O(n)$}, where $n$ is the number of
                 elements to be sorted; but it was conjectured to be {$n
                 \log n + o(n \log n)$}. In this paper we give a
                 construction that proves an asymptotic lower bound of
                 {$1.25 n \log n - O(n \log \log n)$} comparisons for
                 the worst case",
  note =         "Also Technical Report A23/90, University of
                 Saarbr{\"u}cken, Germany",
  refersto =     "\cite{Carlsson87-3}, \cite{Carlsson87-2},
                 \cite{McDiarmid$^+$89}, \cite{Mehlhorn84},
                 \cite{Wegener90}, \cite{Wegener92}, \cite{Williams64}",
}



                             

