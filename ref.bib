@TechReport{BMS_real,
  author =       {C. Burnikel and K. Mehlhorn and }#Schirra#{},
  year =         "1996",
  title =        {\htmladdnormallink{The {LEDA} Class {\em real} Number}{http://www.mpi-sb.mpg.de/\~{}mehlhorn/ftp/LEDA-real.ps}},
  type =         "Technical Report",
  number =       "MPI-I-96-1-001",
  institution =  MPII,
  address =      "Saarbr{\"u}cken",
  anote={\htmladdnormallink{\myurl{www.mpi-sb.mpg.de/~mehlhorn/ftp/LEDA-real.ps}}{http://www.mpi-sb.mpg.de/\~{}mehlhorn/ftp/LEDA-real.ps}},
  abstract =     "We describe the implementation of the LEDA type {\bf
                 real}. Every integer is a real and reals are closed
                 under the operations addition, subtraction,
                 multiplication, division and squareroot. The
                 user--interface is similar to that of the built--in
                 data type {\bf double}. All comparison operators are
                 exact. The implementation of the data type real is
                 based on the LEDA data types {\bf integer} and {\bf
                 bigfloat} which are the types of arbitrary precision
                 integers and floating point numbers, respectively."
}

        


@TechReport{BKbigfloat,
  author =       "C. Burnikel and J. K{\"o}nemann",
  year =         "1996",
  title =        "High--Precision Floating Point Numbers in {LEDA}",
  type =         "Technical Report",
  number =       "MPI-I-96-1-002",
  institution =  MPII,
  address =      "Saarbr{\"u}cken",
  abstract =     "We describe the implementation of the LEDA type {\bf
                 bigfloat}. The standard data type {\em double} for
                 floating point computation is by the IEEE standard
                 restricted to an exponent range of 11 bit and to an
                 significant range of 53 bit. bigfloats generalize {\bf
                 doubles} in a natural way by allowing arbitrarily large
                 exponents and significants. Apart from that, the IEEE
                 standard is followed and extended. bigfloat arithmetic
                 is powerful since rounding mode and length of the
                 significant can be chosen arbitrarily for every single
                 operation."
}

     

